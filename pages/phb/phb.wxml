<view class='bigbox'>
    <image  mode='aspectFill' class='back' src='{{imgServer}}/img/phbimg/beij.png'></image>
    <image class='yxt' src='{{imgServer}}/img/phbimg/yxt.png'></image>

  <view class='dtzs'>总答题数</view>
  <view class='tinum'>{{questionTotal}}</view>
  <view class='correct'>
    <view class='green'></view>
    <text class='cfont'>正确答题数量：</text>
    <view class='cfontnum'>{{okTotal}}</view>
  </view>
  <view class='wrong'>
    <view class='red'></view>
    <text class='wfont'>错误答题数量：</text>
    <view class='wfontnum'>{{errTotal}}</view>
  </view>
  <view class='again'>
    <text bindtap='go'>再来一遍</text>
  </view>
</view>